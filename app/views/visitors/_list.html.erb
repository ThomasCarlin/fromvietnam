<div id="filterrific_results">

  <div>
    <%= page_entries_info whats  %>
  </div>

  <table>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Country</th>
      <th>Registered at</th>
    </tr>
    <% whats.each do |x| %>
      <tr>
        <td><%= link_to(x.name, case_path(x)) %></td>
        <td><%= x.email %></td>
        <td><%= x.country %></td>
        <td><%= x.created_at %></td>
      </tr>
    <% end %>
  </table>
  <%= will_paginate whats %>
</div>

